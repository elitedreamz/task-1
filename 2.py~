result=0
i=0
input_string=raw_input ("Enter the string:")
length=len(input_string)
length=length-1
while length>=0:
    character=ord(input_string[i])			#predefined function to get ASCII of the char
    result=result+((character-64)*pow(26,length))	
    i=i+1
    length=length -1

result = result -1
print "the associated number is:"
print result
